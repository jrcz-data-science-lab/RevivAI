---
import AppLayout from '@/layouts/app-layout.astro';
import { Projects as ProjectsApp } from '@/components/projects/projects';
import NavbarMini from '@/components/navbar-mini.astro';

// Do not render on the server
export const prerender = false;
---

<AppLayout>
	<NavbarMini />
	<ProjectsApp client:only />
	<img src="/logo2.svg" alt="RevivAI" class="w-full absolute top-16 px-16 opacity-10 not-dark:invert-100 z-0 animate-in fade-in duration-[2s]" draggable="false" />
</AppLayout>
